{
	"expo": {
		"name": "leafly",
		"slug": "leafly",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"web": {
			"bundler": "metro",
			"output": "single",
			"favicon": "./assets/images/favicon.png"
		},
		"ios": {
			"usesAppleSignIn": true,
			"supportsTablet": true,
			"bundleIdentifier": "com.viralreach.leafly",
			"infoPlist": {
				"UIBackgroundModes": ["location", "fetch", "remote-notification"],
				"ITSAppUsesNonExemptEncryption": false,
				"UIViewControllerBasedStatusBarAppearance": false,
				"NSLocationWhenInUseUsageDescription": "This app requires access to your location when open.",
				"NSLocationAlwaysAndWhenInUseUsageDescription": "This app requires access to your location even when closed.",
				"NSLocationAlwaysUsageDescription": "This app requires access to your location when open."
			}
		},
		"android": {
			"googleServicesFile": "./google-services.json",
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.viralreach.leafly"
		},
		"plugins": ["expo-router", "expo-font"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "52ae075f-d4c6-44ed-868b-337d935c7f29"
			}
		},
		"owner": "nathan-carter"
	}
}
