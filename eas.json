{
	"cli": {
		"version": ">= 15.0.10",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			}
		},
		"development-device": {
			"developmentClient": true,
			"distribution": "internal"
		},

		"preview": {
			"distribution": "internal"
		},
		"production": {
			"autoIncrement": true
		}
	},
	"submit": {
		"production": {
			"android": {
				"serviceAccountKeyPath": "./serviceAccountKey.json",
				"track": "internal"
			}
		}
	}
}
